{
  "name": "electron-react-ts",
  "version": "0.0.1",
  "description": "ts + react + mobx + electron + antd",
  "main": "main.js",
  "repository": "https://github.com/JokerDr/electron-react-ts",
  "author": "袁庆龙",
  "license": "MIT",
  "scripts": {
    "dev": "node .electron-react/dev-runner.js",
    "pack": "yarn run pack:main && yarn run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --mode=production --progress --colors --config .electron-react/webpack/webpack.config.main.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --mode=production --progress --colors --config .electron-react/webpack/webpack.config.renderer.js",
    "electron-dist:start": "electron ./dist/electron/main.js",
    "electron-build": "cross-env node .electron-react/build.js",
    "electron-build:darwin": "cross-env BUILD_TARGET=darwin node .electron-react/build.js",
    "electron-build:linux": "cross-env BUILD_TARGET=linux node .electron-react/build.js",
    "electron-build:mas": "cross-env BUILD_TARGET=mas node .electron-react/build.js",
    "electron-build:win32": "cross-env BUILD_TARGET=win32 node .electron-react/build.js",
    "electron-builder:linux": "electron-builder --linux deb",
    "electron-builder:win32": "electron-builder --win --x64",
    "electron-forge:win32": "cross-env BUILD_TARGET=win32 electron-forge package",
    "electron-build:clean": "cross-env BUILD_TARGET=clean node .electron-react/build.js",
    "lint": "eslint src --fix --ext .ts,.tsx  && stylelint \"src/**/*.less\" --fix"
  },
  "config": {
    "forge": "./.electron-react/electron-build-tools/forge.config.js"
  },
  "build": {
    "productName": "xxxx",
    "appId": "com.leon.xxxxx",
    "copyright": "xxxx",
    "directories": {
      "output": "./dist/electron-packaged/"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "xxxx"
    },
    "files": [
      "dist/electron/*"
    ],
    "dmg": {},
    "mac": {},
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {}
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "antd": "^3.23.6",
    "mobx": "^5.14.0",
    "mobx-react": "^6.1.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@electron-forge/cli": "^6.0.0-beta.45",
    "@electron-forge/maker-deb": "^6.0.0-beta.45",
    "@svgr/webpack": "^4.3.3",
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "cache-loader": "^4.1.0",
    "cfonts": "^2.4.5",
    "chalk": "^2.4.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "css-modules-typescript-loader": "^3.0.1",
    "del": "^5.1.0",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.0.6",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "multispinner": "^0.2.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "react-router-dom": "^5.1.2",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "ts-import-plugin": "^1.6.1",
    "ts-loader": "^6.2.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.6.4",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "^4.41.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
